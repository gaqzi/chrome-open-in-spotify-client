{
    "manifest_version": 2,
    "name": "Open in Spotify desktop client",
    "description": "Instead of opening open.spotify.com links in the browser they will always be opened in Spotify",
    "version": "1.4.1",
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "*://open.spotify.com/",
        "*://play.spotify.com/"
    ],
    "background": {
        "page": "chrome-background.html"
    },
    "icons": [
        "icon.png"
    ]
}
